<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
	<item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#FF8000" />
            <stroke
                android:width="1dp"
                android:color="#eeeeee" />
        </shape>
        
    </item>
    <item>
        <shape>
            <solid
                android:color="#3D3D3D" />
            <stroke
                android:width="1dp"
                android:color="#eeeeee" />
            
            <padding
                android:left="5dp"
                android:top="3dp"
                android:right="5dp"
                android:bottom="3dp" />
        </shape>
    </item>
</selector>
